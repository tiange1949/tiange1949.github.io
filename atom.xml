    <content type="html"><![CDATA[<div class="table-container"><table><thead><tr><th style="text-align:center"><strong>汉字</strong></th><th style="text-align:center">类型</th><th style="text-align:center">编码长度</th><th style="text-align:center">五笔编码</th></tr></thead><tbody><tr><td style="text-align:center">雇</td><td style="text-align:center">全码</td><td style="text-align:center">四码</td><td style="text-align:center"><kbd>ynwy</kbd></td></tr><tr><td style="text-align:center">诀</td><td style="text-align:center">识别码</td><td style="text-align:center">左右型</td><td style="text-align:center"><kbd>ynwy</kbd></td></tr><tr><td style="text-align:center">慧</td><td style="text-align:center">简码</td><td style="text-align:center">三级</td><td style="text-align:center"><kbd>dhd</kbd></td></tr><tr><td style="text-align:center">幽</td><td style="text-align:center">全码</td><td style="text-align:center">三码</td><td style="text-align:center"><kbd>xxm</kbd></td></tr><tr><td style="text-align:center">柳</td><td style="text-align:center">简码</td><td style="text-align:center">三级</td><td style="text-align:center"><kbd>sqt</kbd></td></tr><tr><td style="text-align:center">唐</td><td style="text-align:center">简码</td><td style="text-align:center">三级</td><td style="text-align:center"><kbd>yvh</kbd></td></tr><tr><td style="text-align:center">赶</td><td style="text-align:center">识别码</td><td style="text-align:center">杂合型</td><td style="text-align:center"><kbd>fhfk</kbd></td></tr><tr><td style="text-align:center">孩</td><td style="text-align:center">全码</td><td style="text-align:center">四码</td><td style="text-align:center"><kbd>bynw</kbd></td></tr><tr><td style="text-align:center">陔</td><td style="text-align:center">全码</td><td style="text-align:center">四码</td><td style="text-align:center"><kbd>bynw</kbd></td></tr><tr><td style="text-align:center">鹫</td><td style="text-align:center">全码</td><td style="text-align:center">四码</td><td style="text-align:center"><kbd>yidg</kbd></td></tr><tr><td style="text-align:center"><strong><font color="#FF0000">筋</font></strong></td><td style="text-align:center">识别码</td><td style="text-align:center">上下型</td><td style="text-align:center"><kbd>telb</kbd></td></tr><tr><td style="text-align:center"><strong><font color="#FF0000">噬</font></strong></td><td style="text-align:center">简码</td><td style="text-align:center">三级</td><td style="text-align:center"><kbd>kta</kbd></td></tr><tr><td style="text-align:center">瞪</td><td style="text-align:center">简码</td><td style="text-align:center">三级</td><td style="text-align:center"><kbd>hwg</kbd></td></tr><tr><td style="text-align:center">哭</td><td style="text-align:center">识别码</td><td style="text-align:center">上下型</td><td style="text-align:center"><kbd>kkdu</kbd></td></tr><tr><td style="text-align:center"><strong><font color="#FF0000">殷</font></strong></td><td style="text-align:center">简码</td><td style="text-align:center">三级</td><td style="text-align:center"><kbd>rvn</kbd></td></tr><tr><td style="text-align:center"><strong><font color="#FF0000">醴</font></strong></td><td style="text-align:center">全码</td><td style="text-align:center">四码</td><td style="text-align:center"><kbd>sgmu</kbd></td></tr><tr><td style="text-align:center">麒</td><td style="text-align:center">全码</td><td style="text-align:center">四码</td><td style="text-align:center"><kbd>ynjw</kbd></td></tr><tr><td style="text-align:center">麟</td><td style="text-align:center">全码</td><td style="text-align:center">四码</td><td style="text-align:center"><kbd>ynjh</kbd></td></tr><tr><td style="text-align:center">泰</td><td style="text-align:center">识别码</td><td style="text-align:center">上下型</td><td style="text-align:center"><kbd>dwiu</kbd></td></tr><tr><td style="text-align:center">磨</td><td style="text-align:center">全码</td><td style="text-align:center">四码</td><td style="text-align:center"><kbd>yssd</kbd></td></tr><tr><td style="text-align:center">靡</td><td style="text-align:center">全码</td><td style="text-align:center">四码</td><td style="text-align:center"><kbd>yssd</kbd></td></tr><tr><td style="text-align:center"><strong><font color="#FF0000">戎</font></strong></td><td style="text-align:center">识别码</td><td style="text-align:center">杂合型</td><td style="text-align:center"><kbd>ade</kbd></td></tr><tr><td style="text-align:center">脩</td><td style="text-align:center">全码</td><td style="text-align:center">四码</td><td style="text-align:center"><kbd>whte</kbd></td></tr><tr><td style="text-align:center">羲</td><td style="text-align:center">简码</td><td style="text-align:center">三级</td><td style="text-align:center"><kbd>ugt</kbd></td></tr><tr><td style="text-align:center"><strong><font color="#FF0000">尧</font></strong></td><td style="text-align:center">全码</td><td style="text-align:center">四码</td><td style="text-align:center"><kbd>atgq</kbd></td></tr></tbody></table></div>]]></content>
    
    
      
      
